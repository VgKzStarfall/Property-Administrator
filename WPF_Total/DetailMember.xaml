<Window x:Class="zPage.DetailMember"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:zPage"
        mc:Ignorable="d"
        Title="DetailMember" Height="400" Width="559">
    <Grid>
        <Label Name="lbName" Content="Label" FontSize="30" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.006,-0.221"/>
        <Label Content="City:" FontSize="20" HorizontalAlignment="Left" Margin="17,89,0,0" VerticalAlignment="Top"/>
        <Label Name="lbCity" Content="Label" FontSize="20" HorizontalAlignment="Left" Margin="200,89,0,0" VerticalAlignment="Top"/>
        <Label Content="Country:" FontSize="20" HorizontalAlignment="Left" Margin="17,155,0,0" VerticalAlignment="Top"/>
        <Label Name="lbCountry" Content="Label" FontSize="20" HorizontalAlignment="Left" Margin="200,155,0,0" VerticalAlignment="Top"/>
        <Label Content="Email:" FontSize="20" HorizontalAlignment="Left" Margin="19,216,0,0" VerticalAlignment="Top"/>
        <Label Name="lbEmail" Content="Label" FontSize="20" HorizontalAlignment="Left" Margin="200,216,0,0" VerticalAlignment="Top"/>
        <Button Name="btnUpdate" Content="Update" FontSize="20" HorizontalAlignment="Left" Margin="41,289,0,0" VerticalAlignment="Top" Height="43" Width="116" Click="btnUpdate_Click"/>
        <Button Name="btnDelete" Content="Delete" FontSize="20" HorizontalAlignment="Left" Margin="228,289,0,0" VerticalAlignment="Top" Height="43" Width="116" Click="btnDelete_Click"/>

    </Grid>
</Window>
